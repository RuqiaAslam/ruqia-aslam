 #include <iostream>
 using namespace std;
 class test
 {
    private:
       int num;
    public:          
        Test()
        {
           num=7;
        }
        void operator --()
        {
           num = num-2;
        }
        void print()
        {
           cout<<" the count is: "<<num;
        }
 };
 
 int main()
 {
     test tt;
     --tt;  // calling of a function "void operator ++()"
     tt.print();
     return O;
 }                                                             
